import java.util.*;
import java.io.*;
import java.math.*;

class Player {
    
    static class Coords{
        private int x,y;
        
        Coords (int x , int y){
            this.x = x;
            this.y = y;
        }  
        
        public int getX(){
            return x;
        }
        
        public int getY(){
            return y;
        }
        
        public boolean attack(ArrayList<Coords> monsters){
            int n=0;
            
            
            Iterator<Coords> it = monsters.iterator();
            while(it.hasNext()){
                
                Coords next = it.next();
                if (Math.abs(next.x - x)<1 || Math.abs(next.y - y)<1){
                	System.out.println("STRIKE");
                	return true;
                }   
                n++;
            }
            
            
            return false;
        }
        
        public boolean move(Coords destination){
            if ((destination.y - y)==0 && (destination.x - x)==0) return false;
            if ((destination.y - y)>0){
                System.out.print("S");
                this.y++;
            }
            else if ((destination.y - y)<0){
                System.out.print("N");
                this.y--;
            }
            
            if ((destination.x - x)>0){
                System.out.print("E");
                this.x++;
            }
            else if ((destination.x - x)<0){
                System.out.print("W");
                this.x--;
            }
            
            System.out.println();
            return true;
            
        }
    }
    
    static Coords getCenter(ArrayList<Coords> monsters){
        int x=0, y=0, n=1;
        
        Iterator<Coords> it = monsters.iterator();
        while(it.hasNext()){
            Coords next = it.next();
            x+= next.getX();
            y+= next.getY();
            System.err.println("Iteration "+n);
            n++;
        }
        
        x = x/n;
        y = y/n;
        
        System.err.println("Got center");
        
        return new Coords(x,y);
        
    }

    public static void main(String args[]) {
        System.err.println("Program Started");
        
        Scanner in = new Scanner(System.in);

        // Read init information from standard input, if any
        int x = in.nextInt();
        int y = in.nextInt();
        
        Coords thor = new Coords (x,y);
        System.err.println("Read Thor coords");
        
        while (true) {
            System.err.println("Infinite loop here");
        
            // Read information from standard input
            int h = in.nextInt();
            int n = in.nextInt();
            
            ArrayList<Coords> monsters = new ArrayList<Coords>();
            for (int i=0; i<n; i++){
                int a = in.nextInt();
                int b = in.nextInt();
                monsters.add(new Coords(a,b));
            }
            
            Coords center = getCenter(monsters);
            System.err.println ("Center coords : "+center.getX()+" , "+ center.getY());
            
            if(!thor.attack()){ //if there's a close monster attack him
            	if (!thor.move(center)) //if the center is away go to it
                	 //otherwise we'll wait
                     System.out.println("WAIT");
                    
            }
            
            
                
                

            // System.err.println("Debug messages...");

        }
    }
}